PROJECT_NAME            = "Golden Gate"
OUTPUT_DIRECTORY        = @DOXYGEN_OUTPUT_DIR@
INPUT                   = @DOXYGEN_INPUTS@ @CMAKE_CURRENT_LIST_DIR@
EXCLUDE_PATTERNS        = *.py
STRIP_FROM_PATH         = @GG_ROOT@
STRIP_FROM_INC_PATH     = @GG_ROOT@
USE_MDFILE_AS_MAINPAGE  = @DOXYGEN_README@
QUIET                   = @DOXYGEN_QUIET@
MULTILINE_CPP_IS_BRIEF  = YES
ENABLE_PREPROCESSING    = YES
MACRO_EXPANSION         = YES
EXPAND_ONLY_PREDEF      = YES
EXPAND_AS_DEFINED       = GG_IMPLEMENTS GG_DECLARE_INTERFACE
INCLUDE_PATH            = @GG_ROOT@
IGNORE_PREFIX           = GG_
TYPEDEF_HIDES_STRUCT    = YES
GENERATE_DOCSET         = YES
DOCSET_FEEDNAME         = "Golden Gate Documentation"
DOCSET_BUNDLE_ID        = com.fitbit.GoldenGate
DOCSET_PUBLISHER_ID     = com.fitbit
DOCSET_PUBLISHER_NAME   = Fitbit
GENERATE_LATEX          = NO
GENERATE_XML            = YES
